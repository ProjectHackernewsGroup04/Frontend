

{{#each stories}}
  <tr>
    <td bgcolor=#fafaf0 valign="top">
      <span>{{inc @index}}</span>
      <div class="arrow-up"></div>
      <a href='/item/{{id}}'><b>{{title}}</b></a>
      {{#if_not url ""}}
        <a href={{url}}>({{url}})</a>
      {{/if_not}}
    </td>
  </tr>
  <tr>
    <td bgcolor=#fafaf0>
      <span>{{score}} point by {{by}}</span>
      <span class="timeago" title="{{time}}">{{time}}</span>
      <span>| hide | past | web |
        <a href="/item/{{id}}"> discuss </a>
      </span>
      <br><br>
    </td>
  </tr>
{{/each}}

<tr>
  <td bgcolor=#fafaf0>
    <a href="/newest/{{max}}" class="btn btn-info"> More </a>
  </td>
</tr>


<script>
window.scrollTo(0, document.body.scrollHeight);
</script>


